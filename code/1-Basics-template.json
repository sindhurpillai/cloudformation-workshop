{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Description": "Creating S3 bucket for CloudFormation Workshop",
    "Resources": {
        "S3Bucket": {
            "Type": "AWS::S3::Bucket",
            "Properties": {
                "BucketName": "s3-workshop"
            }
        }
    },
    "Outputs": {
        "S3BucketName": {
            "Value": {
                "Ref": "S3Bucket"
            },
            "Description": "Name of S3 bucket created by CloudFormation"
        }
    }
}